{
  "name": "infinitycube/quicksite",
  "description": "Slim 4 scaffolding for creating web applications. QuickSite creates a project structure with all essential packages pre-bundled.",
  "type": "project",
  "keywords": [
    "slim",
    "session",
    "doctrine",
    "orm",
    "migrations",
    "twig"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Gopakumar",
      "email": "me@gopumon.in"
    }
  ],
  "require": {
    "php": ">=7.2",
    "bryanjhv/slim-session": "^4.0",
    "doctrine/dbal": "^2.10",
    "doctrine/migrations": "^2.2",
    "doctrine/orm": "^2.7",
    "middlewares/trailing-slash": "^2.0",
    "monolog/monolog": "^2.0",
    "php-di/php-di": "^6.0",
    "slim/http": "^1.0",
    "slim/psr7": "^1.0",
    "slim/slim": "^4.4",
    "slim/twig-view": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "start": "php -S 0.0.0.0:8080 -t public",
    "test": "phpunit"
  }
}
